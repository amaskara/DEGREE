{
    "dataset": "maven",
    "gpu_device": 0,
    "seed": 0,
    "train_file": "./processed_data/maven/train.json",
    "dev_file": "./processed_data/maven/dev.json",
    "test_file": "./processed_data/maven/test.json",
    "finetune_dir": "./data/degree_ed_maven/",
    "train_finetune_file": "./data/degree_ed_maven/train_all.pkl",
    "dev_finetune_file": "./data/degree_ed_maven/dev_all.pkl",
    "test_finetune_file": "./data/degree_ed_maven/test_all.pkl", 
    "vocab_file": "./data/degree_ed_maven/vocab.json", 
    "output_dir": "./outputs/degree_ed_maven/",
    "cache_dir": "./cache",
    "model_name": "facebook/bart-base",
    "input_style": ["event_type_sent", "keywords", "template"],   
    "output_style": ["trigger:sentence"], 
	"n_negative": 15, 
    "max_epoch": 45,
    "warmup_epoch": 5,
    "train_batch_size": 32,
    "eval_batch_size": 12,
    "accumulate_step": 1,
    "learning_rate": 1e-05,
    "weight_decay": 1e-05,
    "grad_clipping": 5.0,
    "beam_size": 1,
    "max_length": 250,
    "max_output_length": 20,
    "ignore_first_header": true 
}
